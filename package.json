{
  "name": "sassify-object",
  "version": "1.0.1",
  "description": "An utility for declaring variables in SASS files.",
  "main": "index.js",
  "scripts": {
    "changelog-major": "build-changelog --major",
    "changelog-minor": "build-changelog --minor",
    "changelog-patch": "build-changelog --patch",
    "cover": "istanbul cover --report cobertura --print detail tape -- test/index.js",
    "lint": "lint-trap",
    "test": "npm run lint -s && npm run cover -s",
    "fast-test": "npm run lint && tape test/index.js",
    "view-cover": "istanbul report html && opn ./coverage/index.html"
  },
  "author": "freeqaz <freeqaz@freeqaz.com> (http://freeqaz.com)",
  "license": "MIT",
  "devDependencies": {
    "build-changelog": "^2.1.2",
    "istanbul": "^0.3.13",
    "lint-trap": "^1.0.1",
    "opn": "^1.0.1",
    "tape": "^4.0.0"
  },
  "dependencies": {
    "xtend": "^4.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freeqaz/sassify-object.git"
  },
  "bugs": {
    "url": "https://github.com/freeqaz/sassify-object/issues"
  },
  "homepage": "https://github.com/freeqaz/sassify-object#readme"
}
